                    <p1:GGroup text="Operation Mode" manifestObj="true" widgetName="ggroup_operation_mode" bounds="16, 9, 517, 73">
                        <p1:GLabel bounds="12, 21, 73, 15" manifestObj="true" text="Device type:" widgetName="glabel_device_type"/>
                        <p1:GCombo bounds="327, 18, 182, 21" description="Device or bus mode kind.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Master: the peripheral operates as the master in a I2S communication, that is, it is the one that provides the clock.&lt;/li&gt;&#13;&#10;&lt;li&gt;Slave: the peripheral operates as the slave in a I2S communication. It needs the clock provided by a master for communication.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Master&#13;&#10;Slave" value="Master" widgetName="gcombo_device_type" toolTipText="Configures USIC peripheral channel to operate as an&#13;&#10;either master or slave device type."/>
                        <p1:GCheck text="Enable Loop back mode" manifestObj="true" widgetName="gcheck_loopback_mode_enable" bounds="13, 50, 194, 16" toolTipText="Serial In or Serial Out signals are not available for external connections if loop back mode is enabled."/>
                    </p1:GGroup>
                    <p1:GGroup bounds="17, 324, 517, 154" manifestObj="true" text="DX2 Input Stage" widgetName="ggroup_input_stages">
                        <p1:GLabel bounds="12, 20, 115, 15" manifestObj="true" text="Input stage:" widgetName="glabel_dx2_input_stage"/>
                        <p1:GCombo bounds="327, 17, 182, 21" description="Input signal chosen in slave mode for the input stage DX2. This setting can&apos;t be changed, as the mandatory SELIN signal is the input." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="WAIN" value="WAIN" widgetName="gcombo_dx2_input_stage_slave" toolTipText="Configures input signal source for the input stage DX2."/>
                        <p1:GCombo bounds="327, 16, 182, 21" description="Input signal chosen for the input stage DX2.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;None: No signal is used&lt;/li&gt;&#13;&#10;&lt;li&gt;Transmit Data Validation: A singal is used for transmit data validation purposes&lt;/li&gt;&#13;&#10;&lt;li&gt;Delay Compensation: The master receives the slave select signal back from the slave&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="None&#13;&#10;Transmit Data Validation&#13;&#10;Delay Compensation" value="None" widgetName="gcombo_dx2_input_stage_master" toolTipText="Configures input signal source for the input stage DX2."/>
                        <p1:GCheck bounds="10, 44, 246, 16" description="Selection inverts the signal polarity of the input signal DX2." manifestObj="true" text="Invert signal polarity of DX2" widgetName="gcheck_enable_invert_dx2_input_stage" toolTipText="Bit field (DX2CR.DPOL)&#13;&#10;Selection inverts the signal polarity of the input signal DX2."/>
                        <p1:GLabel bounds="11, 123, 273, 15" manifestObj="true" text="Type of data validation:" widgetName="glabel_type_transmit_data_validation"/>
                        <p1:GCombo bounds="327, 124, 182, 21" description="Type of transmit data validation used.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Transfer on low (DX2S = 0): Transfer gating logic is used. A transmission of the data word in TBUF can be started if the value of the signal DX2S is equal to zero.&lt;/li&gt;&#13;&#10;&lt;li&gt;Transfer on high (DX2S = 1):  Also the transfer gating logic is used, but in this case, the transmission can be started if the value of the signal DX2S is equal to one.&lt;/li&gt;&#13;&#10;&lt;li&gt;Transfer on event (DX2T): A transfer trigger logic is made use of. Data words transfers are triggered by events.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Transfer On Low (DX2S = 0)&#13;&#10;Transfer On High (DX2S = 1)&#13;&#10;Transfer On Event (DX2T)" value="Transfer On Low (DX2S = 0)" widgetName="gcombo_type_transmit_data_validation" toolTipText="Bit field (TCSR.TDEN / TCSR.TDVTR)&#13;&#10;Selection controls the gating of the transmission&#13;&#10;start of the data word in the transmit buffer TBUF."/>
                        <p1:GLabel bounds="12, 99, 307, 15" manifestObj="true" text="Triggering edge selection for DX2T event:" widgetName="glabel_transmit_data_validation_trigger_edge"/>
                        <p1:GCombo bounds="327, 94, 182, 21" description="This field defines the edge of the DX2T signal to be used for triggering the transmit data validation.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Rising Edge: The rising edge of the DX2T signal is used.&lt;/li&gt;&#13;&#10;&lt;li&gt;Falling Edge: The rising edge of the DX2T signal is used.&lt;/li&gt;&#13;&#10;&lt;li&gt;Both Edges: Both edges of the DX2T signal is used.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="None&#13;&#10;Rising Edge&#13;&#10;Falling Edge&#13;&#10;Both Edges" toolTipText="Selection activates the DX2S signal edge for trigger output DXnT.  " value="None" widgetName="gcombo_transmit_data_validation_trigger_edge"/>
                        <p1:GCheck bounds="10, 69, 190, 16" description="Enables the digital filter for DX2" manifestObj="true" text="Digital filter for DX2" widgetName="gcheck_dx2"/>
                    </p1:GGroup>
                    <p1:GGroup bounds="16, 83, 517, 142" manifestObj="true" text="Frame Format" widgetName="ggroup_frame_format">
                        <p1:GLabel bounds="12, 85, 140, 16" manifestObj="true" text="System word length:" widgetName="glabel_system_wlen"/>
                        <p1:GLabel bounds="12, 24, 170, 16" manifestObj="true" text="Word length:" widgetName="glabel_word_length" toolTipText=""/>
                        <p1:GLabel bounds="12, 116, 173, 16" manifestObj="true" text="Data shift direction:" widgetName="glabel_data_shift_dir"/>
                        <p1:GCombo bounds="327, 115, 182, 21" description="Shift direction of the data words for transmission and reception.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Shift LSB (Least Significant Bit) first: The first data bit of a data word is located at bit position 0.&lt;/li&gt;&#13;&#10;&lt;li&gt;Shift MSB (Most Significant Bit) first: The first data bit of a data word is located at the bit position given by bit field SCTR.WLE&lt;/li&gt;&#13;&#10;&lt;/ul&gt;&#13;&#10;&lt;br&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="LSB&#13;&#10;MSB" value="MSB" widgetName="gcombo_data_shift_dir" toolTipText="Bit field (SCTR.SDIR)&#13;&#10;Configures shift direction of the data words for transmission and reception."/>
                        <p1:GInteger bounds="327, 21, 182, 23" description="This defines the data word length (amount of&#13;&#10;bits that are transferred in each data word) for&#13;&#10;reception and transmission." format="(com.ifx.davex.ui.controls.util.AppUiConstants).DEC" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="16" minValue="1" value="8" widgetName="ginteger_word_length" toolTipText="Bit field (SCTR.WLE + 1)&#13;&#10;Configures number of bits required in each data word."/>
                        <p1:GLabel bounds="12, 55, 170, 16" manifestObj="true" text="Frame length:" widgetName="glabel_frame_length" toolTipText=""/>
                        <p1:GInteger bounds="327, 52, 182, 23" description="This defines how many bits are transferred within a data frame." format="(com.ifx.davex.ui.controls.util.AppUiConstants).DEC" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="64" minValue="1" value="64" widgetName="ginteger_frame_length" toolTipText="Bit field (SCTR.FLE + 1)&#13;&#10;Configures number of bits required in per data frame."/>
                        <p1:GInteger x:Style="NONE" minValue="1" maxValue="64" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" format="(com.ifx.davex.ui.controls.util.AppUiConstants).DEC" manifestObj="true" widgetName="ginteger_system_wlen" bounds="327, 84, 182, 23" value="64" description="If the system word length is longer than the frame length defined by SCTR.FLE, the&#13;&#10;additional bits are transmitted with passive data level (SCTR.PDL). &#13;&#10;If the system word length is smaller than the device frame length, not all LSBs of the transmit data can be transferred.&#13;&#10;"/>
                    </p1:GGroup>
                    <p1:GGroup text="DX1 Input Stage" manifestObj="true" widgetName="ggroup_dx1" bounds="16, 231, 516, 89">
                        	<p1:GLabel manifestObj="true" text="Input stage:" widgetName="glabel_dx1_input_stage" bounds="12, 22, 115, 15"/>
                        	<p1:GCombo description="Input signal chosen in master mode for the input stage DX1.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;None: No signal is used&lt;/li&gt;&#13;&#10;&lt;li&gt;External Frequency Input: used for the baud rate generation. This allows transfers with a frequency that can not be generated internally.&lt;/li&gt;&#13;&#10;&lt;li&gt;Delay Compensation: The master receives the shift clock back from the slave&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="None&#13;&#10;External Frequency Input&#13;&#10;Delay Compensation" value="None" widgetName="gcombo_dx1_input_stage_master" toolTipText="Configures input signal source for the input stage DX1." bounds="326, 21, 183, 21"/>
                        	<p1:GCombo description="Input signal chosen in slave mode for the input stage DX1. This setting can&apos;t be changed, as the mandatory SCLKIN signal is the input." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="SCLKIN" value="SCLKIN" widgetName="gcombo_dx1_input_stage_slave" toolTipText="Configures input signal source for the input stage DX1." bounds="325, 21, 184, 21"/>
                        	<p1:GCheck description="Selection inverts the signal polarity of the input signal DX1." manifestObj="true" text="Invert signal polarity of DX1" widgetName="gcheck_enable_invert_dx1_input_stage" toolTipText="Bit field (DX1CR.DPOL)&#13;&#10;Selection inverts the signal polarity of the input signal DX1." bounds="12, 46, 246, 16"/>
                        	<p1:GCheck bounds="11, 68, 184, 16" description="Enables the digital filter for DX1" manifestObj="true" text="Digital filter for DX1" widgetName="gcheck_dx1"/>
                        </p1:GGroup>
