        <p1:GTab text="Baudrate Generator Settings" visible="true" enabled="true" manifestObj="true" widgetName="gtab_baudrate">
            <p1:GTab.control>
                <p1:Tab widgetName="composite_42">
                    <p1:GGroup bounds="16, 72, 517, 389" manifestObj="true" text="Baud Rate Generator" widgetName="ggroup_baud_rate">
                        <p1:GLabel bounds="13, 49, 171, 15" manifestObj="true" text="Desired bus speed [kHz]:" widgetName="glabel_desired_baud_rate"/>
                        <p1:GLabel bounds="13, 80, 171, 15" manifestObj="true" text="Divider factor:" widgetName="glabel_divider_factor"/>
                        <p1:GInteger bounds="312, 72, 166, 23" description="This field indicates the ratio between the input frequency fPPP and the divider frequency fPDIV." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="1024" minValue="1" value="1" widgetName="ginteger_divider_factor_internal_clk" enabled="false" toolTipText="Bit field (BRG.PDIV + 1)&#13;&#10;Configures ratio between the input frequency fPPP&#13;&#10;and the divider frequency fPDIV."/>
                        <p1:GLabel bounds="12, 325, 171, 15" manifestObj="true" text="Actual bus speed [kHz]:" widgetName="glabel_actual_baud_rate"/>
                        <p1:GFloat bounds="312, 322, 161, 21" description="Indicates the actual baud that can be achieved with the device. This value is calculated based on the entered value of desired baud rate." enabled="false" format="*,2" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="POSITIVE_INFINITY" minValue="NEGATIVE_INFINITY" value="19.2" widgetName="gfloat_actual_baud_rate" x:Style="BORDER" toolTipText="Indicates the actual baud that can be achieved with the device.&#13;&#10;This value is calculated based on the entered value of desired baud rate."/>
                        <p1:GFloat bounds="313, 44, 163, 21" description="Allows to configure the desired baud rate for the SPI communication. The value should be specified in baud. The maximum value of the baud rate should be calculated based on peripheral clock and the oversampling settings. Max baud = (fPERIPH * 1023)/(1024 * oversampling)." format="*,2" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="60000" minValue="0.1" value="19.2" widgetName="gfloat_desired_baud_rate" x:Style="BORDER" toolTipText="Configure the desired baud rate for the SPI communication."/>
                        <p1:GLabel bounds="13, 26, 110, 15" manifestObj="true" text="Clock:" widgetName="glabel_clock_selection"/>
                        <p1:GCombo bounds="313, 18, 164, 21" description="This field defines the frequency fPIN, input to the dividers of the BRG.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Internal Source (FD): The fractional divider frequency fFD is selected.&lt;/li&gt;&#13;&#10;&lt;li&gt;External Source (DX1T): The trigger signal DX1T defines fPIN.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Internal Source (FD)&#13;&#10;External Source (DX1T)" value="Internal Source (FD)" widgetName="gcombo_clock_selection" toolTipText="Bit field (BRG.CLKSEL)&#13;&#10;Configures baud rate generator input frequency (fPIN) source."/>
                        <p1:GLabel bounds="13, 51, 154, 15" manifestObj="true" text="Edge selection:" widgetName="glabel_external_clock_edge"/>
                        <p1:GCombo bounds="313, 45, 164, 21" description="This field selects which edge of the external source signal (DX1S) must be used to generate the clock signal (DX1T)&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;Rising Edge: The rising edge of the external signal generates the clock signal.&lt;/li&gt;&#13;&#10;&lt;li&gt;Falling Edge: The falling edge of the external signal generates the clock signal.&lt;/li&gt;&#13;&#10;&lt;li&gt;Both Edges: Both edges of the external signal generate the clock signal.&lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="Rising Edge&#13;&#10;Falling Edge&#13;&#10;Both Edges" value="Rising Edge" widgetName="gcombo_external_clock_edge" toolTipText="Bit field (DX1CR.CM)&#13;&#10;Selection activates the DX1S signal edge for trigger output DXnT."/>
                        <p1:GImage visible="true" manifestObj="true" widgetName="gimage_baud_rate" bounds="10, 126, 465, 190" path="Uimodel/I2S_CONFIG_BRG_internal_clk.png" x:Style="BORDER"/>
                        <p1:GInteger bounds="313, 73, 164, 23" description="This field indicates the ratio between the input frequency fPPP and the divider frequency fPDIV." mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" maxValue="1024" minValue="1" value="1" widgetName="ginteger_divider_factor_external_clk" toolTipText="Bit field (BRG.PDIV + 1)&#13;&#10;Configures ratio between the input frequency fPPP and the divider frequency fPDIV." enabled="false"/>
                        <p1:GCheck bounds="12, 354, 286, 16" description="On selecting this option, the capture mode timer is enabled." manifestObj="true" text="Enable the capture mode timer" widgetName="gcheck_enable_capture_mode_timer" toolTipText="Bit field (BRG.TMEN)&#13;&#10;On selecting this option, the capture mode timer is enabled."/>
                        <p1:GLabel text="Step value:" visible="true" manifestObj="true" widgetName="glabel_step" bounds="13, 100, 106, 15"/>
                        <p1:GInteger x:Style="NONE" minValue="0" maxValue="1024" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" format="(com.ifx.davex.ui.controls.util.AppUiConstants).DEC" manifestObj="true" widgetName="ginteger_step" bounds="312, 100, 165, 23" value="0" enabled="false" description="In normal divider mode STEP contains the reload value for RESULT after RESULT has reached 3FFH. &lt;br&gt;&#13;&#10;In fractional divider mode STEP defines the value added to RESULT with each input clock cycle.&#13;&#10;"/>
                    </p1:GGroup>
                    <p1:GLabel bounds="18, 9, 478, 31" manifestObj="true" text="Shift clock signal (SCLKOUT) setting with respect to the shift clock (SCLK) signal from the&#13;&#10;Baud Rate Generator: " widgetName="glabel_shift_clock_setting"/>
                    <p1:GCombo bounds="18, 44, 358, 21" description="Configures the required delay and signal polarity level for the shift clock output signal SCLKOUT with respect to SCLK.&lt;br&gt;&#13;&#10;&lt;ul&gt;&#13;&#10;&lt;li&gt;No Delay, No Polarity Inversion: SCLKCFG = 00B, SCLKOUT equals SCLK.&lt;/li&gt;&#13;&#10;&lt;li&gt;No Delay, Polarity Inversion:  SCLKCFG = 01B, SCLKOUT inverted to SCLK.&lt;/li&gt;&#13;&#10;&lt;li&gt;Delayed By 1/2 Shift Clock Period, No Polarity Inversion: SCLKCFG = 10B, SCLKOUT delayed / shifted by 1/2 SCLK Clock Period. &lt;/li&gt;&#13;&#10;&lt;li&gt;Delayed By 1/2 Shift Clock Period, Polarity Inversion: SCLKCFG = 11B, SCLKOUT delayed / shifted by 1/2 SCLK Clock Period and inverted to SCLK. &lt;/li&gt;&#13;&#10;&lt;/ul&gt;" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="No Delay, No Polarity Inversion&#13;&#10;No Delay, Polarity Inversion&#13;&#10;Delayed By 1/2 Shift Clock Period, No Polarity Inversion&#13;&#10;Delayed By 1/2 Shift Clock Period, Polarity Inversion" value="No Delay, No Polarity Inversion" widgetName="gcombo_shift_clock_setting" toolTipText="Bit field (BRG.SCLKCFG )&#13;&#10;Configures the required delay and signal polarity level&#13;&#10;for the shift clock output signal SCLKOUT with respect to SCLK."/>
                    <p1:GMessage bounds="19, 470, 510, 35" isError="false" manifestObj="true" text="For baudrate generation to run on external frequency, the DX1 input stage needs to be set to External Frequency Input." widgetName="gmessage_baud_rate_external_clock_source"/>
                </p1:Tab>
            </p1:GTab.control>
        </p1:GTab>
        <p1:GTab text="Events Settings" visible="true" enabled="true" manifestObj="true" widgetName="gtab_events">
            <p1:GTab.control>
                <p1:Tab widgetName="composite_33">
                    <p1:GGroup text="Protocol-Specific Events" manifestObj="true" widgetName="ggroup_protocol_events" bounds="18, 16, 511, 190">
                        <p1:GCheck bounds="10, 81, 208, 16" description="On selecting this option, it indicates that a rising edge of the WA output signal has been generated. &#13;&#10;" manifestObj="true" text="WA raising edge event" widgetName="gcheck_raisingedge_event" toolTipText="On selecting this option, MSLS event is enabled and&#13;&#10;indicates that the MSLS signal has changed its state."/>
                        <p1:GCheck bounds="11, 21, 223, 16" description="On selecting this option, the DX2T event is enabled. &lt;br&gt;&#13;&#10;In IIS slave mode, an activation of DX2T generates a protocol interrupt if PCR.DX2TIEN = 1.&#13;&#10;" manifestObj="true" text="DX2T event" widgetName="gcheck_dx2_event" toolTipText="On selecting this option, the DX2T event is enabled.&#13;&#10;In slave mode, indicates that the slave select signal has&#13;&#10;changed its state."/>
