                    	<p1:GCombo mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" widgetName="gcombo_mclkout_output_master" bounds="326, 41, 174, 21" options="None&#13;&#10;Master Time Base" value="None" description="Master clock out output in master mode"/>
                    	<p1:GCheck text="Invert signal polarity of output signals" manifestObj="true" widgetName="gcheck_enable_invert_outputs" bounds="14, 74, 291, 16" description="If enabled, the clock outputs have the inverted polarity.&#13;&#10;"/>
                    	<p1:GCombo bounds="326, 42, 174, 21" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="None&#13;&#10;Independent Clock Output" value="None" widgetName="gcombo_mclkout_output_slave" description="Master clock out output in slave mode"/>
                    	<p1:GCombo bounds="326, 14, 174, 21" mandatory="(com.ifx.davex.ui.controls.util.AppUiConstants).FALSE" manifestObj="true" options="None&#13;&#10;Independent Clock Output" value="None" widgetName="gcombo_sclkout_output_slave" description="Shift clock out output in slave mode"/>
                    </p1:GGroup>
                    <p1:GMessage manifestObj="true" text="For the delay compensation in master mode to work, the &quot;Delay Compensation&quot; input has to be selected in the DX1 Input Stage." widgetName="gmessage_delay_compensation_method_master" bounds="18, 582, 513, 39"/>
                    <p1:GMessage manifestObj="true" text="Using the &quot;No delay, polarity inversion&quot; setting for the Shift Clock, the polarity inversion needs to be inverted for the DX1 input stage." widgetName="gmessage_invert_dx1_input_stage" bounds="18, 582, 510, 39"/>
                    <p1:GMessage text="For DX2 event to be generated, select the trigger edge in &quot;DX2 Input Stage&quot; settings. If the selection is &quot;None&quot;, NO event would be generated." visible="true" manifestObj="true" isError="false" widgetName="gmessage_dx2_event_general_settings_page" bounds="20, 584, 522, 34"/>
                </p1:Tab>
            </p1:GTab.control>
